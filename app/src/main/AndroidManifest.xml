<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.masum.adapters" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListViewNArrayAdapter"
            android:label="@string/title_activity_list_view_narray_adapter" >
        </activity>
        <activity
            android:name=".GrideViewWithBaseAdapter"
            android:label="@string/title_activity_gride_view_with_base_adapter" >
        </activity>
        <activity
            android:name=".MyDialog"
            android:label="@string/title_activity_my_dialog" >
        </activity>
        <activity
            android:name=".SpinnerAdapter"
            android:label="@string/title_activity_spinner_adapter" >
        </activity>
    </application>

</manifest>
